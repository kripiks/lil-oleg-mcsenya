    window.GOALS_DATA = {"307109":{"id":"307109","name":"\u0422\u0440\u0435\u043d\u0438\u0440\u043e\u0432\u043a\u0430 &quot;\u043f\u044f\u0442\u043d\u0438\u0446\u0430&quot; \u0441\u043f\u0438\u043d\u0430 \u0438 \u0442\u0440\u0438\u0446\u0435\u043f\u0441","descr":"<p>\u041f\u044f\u0442\u043d\u0438\u0447\u043d\u0430\u044f \u0442\u0440\u0435\u043d\u0438\u0440\u043e\u0432\u043a\u0430<\/p>\n\n<p>\u0421\u043f\u043b\u0438\u0442 \u043d\u0430 <strong>\u0441\u043f\u0438\u043d\u0430<\/strong> \u0438 <strong>\u0442\u0440\u0438\u0446\u0435\u043f\u0441<\/strong><\/p>\n\n<p>1\u0439 \u043f\u043e\u0434\u0445\u043e\u0434: \u043c\u0430\u043b\u044b\u0439 \u0432\u0435\u0441 15 \u043f\u043e\u0432\u0442\u043e\u0440\u0435\u043d\u0438\u0439 (\u0434\u043b\u044f \u0440\u0430\u0437\u043e\u0433\u0440\u0435\u0432\u0430)<\/p>\n\n<p>\u043f\u0435\u0440\u0435\u0440\u044b\u0432 1 \u043c\u0438\u043d.<\/p>\n\n<p>2\u0439 \u043f\u043e\u0434\u0445\u043e\u0434: \u0440\u0430\u0431\u043e\u0447\u0438\u0439 \u0432\u0435\u0441 7-8 \u043f\u043e\u0432\u0442\u043e\u0440\u0435\u043d\u0438\u0439<\/p>\n\n<p>\u043f\u0435\u0440\u0435\u0440\u044b\u0432 2 \u043c\u0438\u043d.<\/p>\n\n<p>3\u0439 \u043f\u043e\u0434\u0445\u043e\u0434: \u0440\u0430\u0431\u043e\u0447\u0438\u0439 \u0432\u0435\u0441 7-8 \u043f\u043e\u0432\u0442\u043e\u0440\u0435\u043d\u0438\u0439<\/p>\n\n<p>\u043f\u0435\u0440\u0435\u0440\u044b\u0432 2 \u043c\u0438\u043d.<\/p>\n\n<p>4\u0439 \u043f\u043e\u0434\u0445\u043e\u0434: \u0440\u0430\u0431\u043e\u0447\u0438\u0439 \u0432\u0435\u0441 \u043c\u0430\u043a\u0441\u0438\u043c\u0443\u043c \u043f\u043e\u0432\u0442\u043e\u0440\u0435\u043d\u0438\u0439 (\u043c\u0430\u043a\u0441\u0438\u043c\u0443\u043c \u0434\u043b\u044f \u0442\u043e\u0433\u043e \u0447\u0442\u043e \u0431\u044b \u043f\u0440\u043e\u0432\u0435\u0440\u0438\u0442\u044c \u0441\u0442\u043e\u0438\u0442 \u043b\u0438 \u0443\u0432\u0435\u043b\u0438\u0447\u0438\u0442\u044c \u0440\u0430\u0431\u043e\u0447\u0438\u0439 \u0432\u0435\u0441)<\/p>\n\n<p>\u043f\u0435\u0440\